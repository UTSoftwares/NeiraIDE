{
  "name": "neiraide",
  "version": "0.0.1",
  "description": "NeiraIDE is an IDE to write and run python code in web browser/electron using Skulpt",
  "main": "electron.js",
  "directories": {
    "doc": "doc"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "get-json": "^1.0.1"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "electron": "^11.5.0"
  },
  "scripts": {
    "start": "electron-forge start",
    "test": "test",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RaccoonSoftwares/neiraide.git"
  },
  "keywords": [
    "neira",
    "ide",
    "python",
    "neiraide"
  ],
  "author": "dima13230",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/RaccoonSoftwares/neiraide/issues"
  },
  "homepage": "https://github.com/RaccoonSoftwares/neiraide#readme",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "neiraide"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
